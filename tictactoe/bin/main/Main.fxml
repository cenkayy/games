<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="186.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainController">
   <children>
      <VBox prefHeight="186.0" prefWidth="345.0">
         <children>
            <Button fx:id="btnPlayer" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#btnPlayerOnClick" prefHeight="93.0" prefWidth="345.0" text="PLAYER 1 VS PLAYER 2">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
            <Button fx:id="btnComputer" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#btnComputerOnClick" prefHeight="93.0" prefWidth="345.0" text="PLAYER VS COMPUTER">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
